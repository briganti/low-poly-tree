[ignore]
.*/node_modules/.*
.*webpack\.config\.js
.*/vendors/.*

[include]

[libs]
flow-typed

[options]
all=true
module.file_ext=.js
module.name_mapper='^vendors/stats.min$' -> '<PROJECT_ROOT>/flow/stub/stats.min.js'
module.name_mapper='^demo\(\/?.*\)$' -> '<PROJECT_ROOT>/demo/js/\1'
module.name_mapper='^src\(\/?.*\)$' -> '<PROJECT_ROOT>/src/\1'

